<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kurovszky.robin.unicalendar">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.orm.SugarApp">
        <activity android:name="kurovszky.robin.unicalendar.UnireqMainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kurovszky.robin.unicalendar.AddReq"
            android:label="@string/title_activity_add_req"
            android:parentActivityName="kurovszky.robin.unicalendar.UnireqMainActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="kurovszky.robin.unicalendar.UnireqMainActivity" />
        </activity>
    </application>

</manifest>